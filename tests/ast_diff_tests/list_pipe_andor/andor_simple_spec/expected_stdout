compound_list
    ast_list_item (sep=\n)
        ast_andor_item (negate=false, sep=&&)
            ast_pipe_item
                ast_command
                    ast_simple_command (echo 111)
        ast_andor_item (negate=false, sep=||)
            ast_pipe_item
                ast_command
                    ast_simple_command (echo 222)
        ast_andor_item (negate=false, sep=none)
            ast_pipe_item
                ast_command
                    ast_simple_command (echo 333)
